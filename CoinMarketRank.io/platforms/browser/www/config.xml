<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coinmarketrank.io" version="1.1.12" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>CoinMarketRank.io</name>
    <description>
        A new way to see Coin Market Capitalization Rankings bitcoin, ethereum, ripple, BAT, EOS, Stellar,...
    </description>
    <author email="info@coinmarketrank.io" href="http://coinmarketrank.io">
        MrAddon
    </author>
    <content src="mobile/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
