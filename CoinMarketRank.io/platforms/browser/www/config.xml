<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coinmarketrank.io" version="1.1.22" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>CoinMarketRank.io</name>
    <description>
        A new way to see Coin Market Capitalization Rankings bitcoin, ethereum, ripple, BAT, EOS, Stellar,...
    </description>
    <author email="info@coinmarketrank.io" href="http://coinmarketrank.io">
        MrAddon
    </author>
    <content src="mobile/index.html" />
    <icon height="76" platform="ios" src="images/CoinMarketRank.io.square.76.png" width="76" />
    <icon height="152" platform="ios" src="images/CoinMarketRank.io.square.152.png" width="152" />
    <icon height="120" platform="ios" src="images/CoinMarketRank.io.square.120.png" width="120" />
    <icon src="images/CoinMarketRank.io.square.png" />
    <access origin="https://*/*" />
    <access origin="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="http://*/*" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
